{
    "name": "mobsite/nova-texteditor",
    "description": "A Laravel TextEditor Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Mobsite\\TextEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mobsite\\TextEditor\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
